{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "flow",
      "camelcase": "flow",
      "title": "flowlang parser",
      "scope": "source.flow",
      "file-types": [
        "flow"
      ],
      "injection-regex": "^flow$",
      "class-name": "TreeSitterFlow"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Flowlangparser grammar for tree-sitter",
    "authors": [
      {
        "name": "David Hasselb√∂ck"
      }
    ],
    "links": {
      "repository": "https://github.com/david-hass/lsp-introduction"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
